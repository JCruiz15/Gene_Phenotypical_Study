model.pvalue <- summary(res.cox)$waldtest["pvalue"]
if(model.pvalue < 0.005 & res.zph$table[3] > 0.05){
output <- rbind(output, c(length(colnames(ex)), toString(colnames(ex)), model.pvalue))
}
}
